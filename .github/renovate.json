{
  "extends": ["config:base", ":enablePreCommit"],
  "recreateClosed": true,
  "rangeStrategy": "update-lockfile",
  "enabledManagers": ["poetry"],
  "lockFileMaintenance": {
    "enabled": true,
    "automerge": true,
    "schedule": ["before 4am on monday"]
  },
  "labels": ["dependencies", "RenovateBot"]
}
